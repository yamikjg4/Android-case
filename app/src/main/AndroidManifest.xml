<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.food">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Food"
        tools:replace="android:theme">
        <activity android:name=".customer.orderdish" />
        <activity android:name=".customer.PendingOrders" />
        <activity android:name=".customer.PayableOrders" />
        <activity android:name=".Adminorder.update_deletedish" /> <!-- <activity android:name=".Adminorder.update_deletedish" /> -->
        <activity android:name=".Adminorder.Postdish" />
        <activity android:name=".Admin" />
        <activity android:name=".customer_panel" />
        <activity android:name=".Forgot" />
        <activity android:name=".singup" />
        <activity android:name=".MainMenu" />
        <activity android:name="com.theartofdev.edmodo.cropper.CropImageActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>